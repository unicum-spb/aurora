{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -p 3000:3000 -d backend",
    "migrate": "node ./dist/migrate",
    "start": "nodemon -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.2.1",
    "@loopback/boot": "^1.5.10",
    "@loopback/context": "^1.23.4",
    "@loopback/core": "^1.10.6",
    "@loopback/openapi-v3": "^1.10.0",
    "@loopback/repository": "^1.15.3",
    "@loopback/rest": "^1.22.0",
    "@loopback/rest-explorer": "^1.4.3",
    "@loopback/security": "^0.1.6",
    "@loopback/service-proxy": "^1.3.10",
    "bcryptjs": "^2.4.3",
    "beautiful-dom": "^1.0.7",
    "cheerio": "^1.0.0-rc.3",
    "cyrillic-to-translit-js": "^3.0.1",
    "dayjs": "^1.8.17",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.2.2",
    "kv-redis": "^0.1.3",
    "lodash": "^4.17.15",
    "loopback-connector-mongodb": "^5.2.0",
    "multer": "^1.4.2",
    "node-html-parser": "^1.1.16",
    "python-shell": "^1.0.8"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.15",
    "@loopback/eslint-config": "^4.1.3",
    "@loopback/testlab": "^1.9.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.13",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.144",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.0",
    "source-map-support": "^0.5.13",
    "typescript": "~3.6.4"
  }
}
